#include<string.h>
#include<ctype.h>
#include<stdio.h>
int main()
{
	char s[20],str[20];
	int k,i,j;
	printf("enter a string");
	scanf("%[^\n]",s);
	for(i=0,k=0; s[i]!='\0'; i++)
	{
		if(isalnum(s[i]))
		{
			str[k]=tolower(s[i]);
			k++;
		}
	}
	int compare=1;
	for(i=0,j=k-1; i<j; i++,j--)
	{
		if(str[i]!=str[j])
		{
			compare=0;
			break;
		}
    }
    if(compare ==1)
     printf("true");
    else 
     printf("false");
    return 0;
}
